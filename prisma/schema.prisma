generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique(map: "User_email_key")
  age   Int
}

model usersdata {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique(map: "UsersData_email_key")
  age   Int
}

model admin_data {
  z_id  String @id @db.VarChar(255)
  name  String @db.VarChar(255)
  email String @db.VarChar(255)
  age   String @db.VarChar(255)
}

model user_management {
  z_id                   String  @id @db.VarChar(100)
  userid                 String? @db.VarChar(100)
  first_name             String? @db.VarChar(100)
  middle_name            String? @db.VarChar(100)
  last_name              String? @db.VarChar(100)
  gender                 String? @db.VarChar(30)
  email                  String? @db.VarChar(255)
  password               String? @db.VarChar(100)
  contact_no             String? @db.VarChar(10)
  role                   String? @db.VarChar(20)
  status                 String? @db.VarChar(20)
  subject_specialization String? @db.VarChar(555)
  class_assigned         String? @db.VarChar(100)
  teacher_id             String? @db.VarChar(100)
  admin_id               String? @db.VarChar(100)
  joining_date           String? @db.VarChar(10)
  qualification          String? @db.VarChar(255)
  enrollment_no          String? @db.VarChar(50)
  date_of_birth          String? @db.VarChar(10)
  standard               String? @db.VarChar(10)
  section                String? @db.VarChar(100)
  parent_id              String? @db.VarChar(100)
  admission_date         String? @db.VarChar(10)
  children_id            String? @db.VarChar(100)
  occupation             String? @db.VarChar(100)
  address                String? @db.Text
  nationality            String? @db.VarChar(50)
  cdate                  String? @db.VarChar(100)
  ctime                  String? @db.VarChar(100)
  udate                  String? @db.VarChar(100)
  utime                  String? @db.VarChar(100)
}

model student_registration {
  z_id                  String  @id @db.VarChar(100)
  first_name            String? @db.VarChar(100)
  middle_name           String? @db.VarChar(100)
  last_name             String? @db.VarChar(100)
  adhar_no              String? @db.VarChar(100)
  gender                String? @db.VarChar(100)
  email                 String? @db.VarChar(100)
  date_of_birth         String? @db.VarChar(100)
  contact_no            String? @db.VarChar(100)
  address               String? @db.Text
  previous_school       String? @db.Text
  highest_qualification String? @db.Text
  percentage            String? @db.VarChar(100)
  entrance_exam_score   String? @db.VarChar(100)
  parent_name           String? @db.VarChar(100)
  parent_contact_no     String? @db.VarChar(100)
  parent_email          String? @db.VarChar(100)
  relationship          String? @db.VarChar(100)
  profile_image         String? @db.Text
  cdate                 String? @db.VarChar(100)
  ctime                 String? @db.VarChar(100)
  udate                 String? @db.VarChar(100)
  utime                 String? @db.VarChar(100)
  student_id            String? @db.VarChar(100)
}

model attendance {
  z_id                 String  @id @db.VarChar(100)
  date                 String? @db.VarChar(100)
  student_name         String? @db.VarChar(300)
  roll_no              String? @db.VarChar(100)
  standard             String? @db.VarChar(100)
  division             String? @db.VarChar(100)
  subject              String? @db.VarChar(100)
  time_in              String? @db.VarChar(100)
  time_out             String? @db.VarChar(100)
  attendance_status    String? @db.VarChar(100)
  attendance_marked_by String? @db.VarChar(200)
  cdate                String? @db.VarChar(100)
  ctime                String? @db.VarChar(100)
  udate                String? @db.VarChar(100)
  utime                String? @db.VarChar(100)
}

model admission_form {
  z_id                     String  @id @db.VarChar(100)
  student_name             String? @db.VarChar(200)
  adhar_number             String? @db.VarChar(100)
  pan_number               String? @db.VarChar(100)
  email                    String? @db.VarChar(100)
  profile_img              String? @db.VarChar(200)
  contact_number           String? @db.VarChar(100)
  date_of_birth            String? @db.VarChar(100)
  gender                   String? @db.VarChar(100)
  address                  String? @db.VarChar(100)
  city                     String? @db.VarChar(100)
  state                    String? @db.VarChar(100)
  pincode                  String? @db.VarChar(100)
  nationality              String? @db.VarChar(100)
  course_applied           String? @db.VarChar(200)
  education_qualification  String? @db.VarChar(100)
  percentage               String? @db.VarChar(100)
  bank_name                String? @db.VarChar(100)
  bank_account_holder_name String? @db.VarChar(200)
  bank_branch              String? @db.VarChar(100)
  account_number           String? @db.VarChar(100)
  ifsc_code                String? @db.VarChar(100)
  adhar_card_img           String? @db.VarChar(200)
  pan_card_img             String? @db.VarChar(200)
  prev_marksheet           String? @db.VarChar(200)
  passbook                 String? @db.VarChar(200)
  guardian_name            String? @db.VarChar(100)
  guardian_contact         String? @db.VarChar(100)
  admission_status         String? @db.VarChar(100)
  submission_date          String? @db.VarChar(100)
  admission_fee            String? @db.VarChar(100)
  cdate                    String? @db.VarChar(100)
  ctime                    String? @db.VarChar(100)
  udate                    String? @db.VarChar(100)
  utime                    String? @db.VarChar(100)
}
