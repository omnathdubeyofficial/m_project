  class_assigned         String? @db.VarChar(100)
  teacher_id             String? @db.VarChar(100)
  admin_id               String? @db.VarChar(100)
  joining_date           String? @db.VarChar(10)
  qualification          String? @db.VarChar(255)
  enrollment_no          String? @db.VarChar(50)
  date_of_birth          String? @db.VarChar(10)
  standard               String? @db.VarChar(10)
  section                String? @db.VarChar(100)
  parent_id              String? @db.VarChar(100)
  admission_date         String? @db.VarChar(10)
  children_id            String? @db.VarChar(100)
  occupation             String? @db.VarChar(100)
  address                String? @db.Text
  nationality            String? @db.VarChar(50)
  cdate                  String? @db.VarChar(100)
  ctime                  String? @db.VarChar(100)
  udate                  String? @db.VarChar(100)
  utime                  String? @db.VarChar(100)
  profile_img            String? @db.VarChar(200)
  adhar_card_front_img   String? @db.VarChar(200)
  adhar_card_back_img    String? @db.VarChar(200)
  pan_card_img           String? @db.VarChar(200)
